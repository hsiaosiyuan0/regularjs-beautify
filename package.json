{
  "name": "regularjs-beautify",
  "version": "0.0.1",
  "description": "Beautify the template content of Regularjs",
  "main": "lib/index.js",
  "repository": "https://github.com/hsiaosiyuan0/regularjs-beautify.git",
  "author": "hsiaosiyuan0",
  "license": "MIT",
  "scripts": {
    "build": "babel src -d lib --extensions \".ts\" --source-maps inline",
    "dev": "node lib/index.js"
  },
  "dependencies": {
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/debug": "^4.1.5",
    "@types/node": "^12.11.7",
    "typescript": "^3.6.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
